{
  "expo": {
    "name": "motionary-mobile-app",
    "slug": "motionary-mobile-app",
    "scheme": "motionary-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": ""
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      "expo-video"
    ]
  }
}
